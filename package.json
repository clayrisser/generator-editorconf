{
  "name": "generator-editorconf",
  "version": "0.0.4",
  "description": "ðŸŽ© Yeoman generator for editorconfig",
  "keywords": [
    "yeoman-generator",
    "generator",
    "yo",
    "yeoman",
    "git",
    "github",
    "readme",
    "changelog",
    "project",
    "license",
    "contributing"
  ],
  "main": "generators/app/index.js",
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "npm run clean && babel src -d generators/app",
    "clean": "rm -rf demo generators/*/*.js",
    "link": "npm link && npm link generator-editorconf",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "prepublish": "npm run build",
    "start": "npm run clean && npm run lint && npm run build && yo --destination=demo editorconf",
    "unlink": "npm unlink generator-editorconf"
  },
  "dependencies": {
    "idempotent-babel-polyfill": "0.0.2",
    "lodash": "^4.17.5",
    "yeoman-generator": "^1.0.0",
    "yo-base-prompts": "^0.1.3",
    "yo-option-or-prompt": "^0.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "yo": "^2.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4"
          }
        }
      ],
      "stage-2"
    ]
  },
  "author": {},
  "homepage": "https://github.com/codejamninja/generator-editorconf",
  "license": "MIT",
  "repository": "https://github.com/codejamninja/generator-editorconf",
  "eslintIgnore": [
    "generators",
    "demo"
  ]
}
